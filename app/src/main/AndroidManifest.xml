<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="mobile.com.prototype_socialapp">

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"></uses-permission>
    <uses-permission android:name="android.permission.INTERNET" ></uses-permission>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar">
        <activity android:name=".Activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activity.SignUp_Activity" />
        <activity android:name=".Activity.Main_Category_Activity" />
        <activity android:name=".Activity.Board_Action_Activity" />
        <activity android:name=".Activity.register_board" />
        <activity android:name=".Activity.Personal_activity" />
        <activity android:name=".Activity.user_info_modification" />
        <activity android:name=".Activity.my_board" />
        <activity android:name=".Activity.show_text" />
        <activity android:name=".Activity.text_modification"></activity>
    </application>

</manifest>